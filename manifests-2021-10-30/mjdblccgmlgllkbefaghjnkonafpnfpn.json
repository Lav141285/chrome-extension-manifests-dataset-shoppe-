---
slug: what-font
name: What Font
publisher: Designer Tools
rating: 3
rating_count: 2
user_count: 5,000+
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "What Font",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Lets you know what font is used on any website.",
  "icons": {
    "128": "i/fonticons.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/chrome.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://typekit.com; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "128": "i/logo_1.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js",
        "js/main.js",
        "js/start.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "css/options.css"
  ],
  "options_page": "css/options.html",
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
