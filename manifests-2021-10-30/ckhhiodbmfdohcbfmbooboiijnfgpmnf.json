  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "https://www.icashback.ro/*"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/bootstrap.bundle.min.js",
        "js/content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/content-script.js",
    "css/fix.css",
    "icons/logo.png",
    "icons/coupon.svg"
  ],
  "content_security_policy": "script-src 'self' ; object-src 'self'"
