---
slug: rapid7-appsec-plugin
name: Rapid7 AppSec plugin
publisher: chris_laughlin
rating: 5
rating_count: 2
user_count: 4,000+
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "author": "Rapid7",
    "name": "Rapid7 AppSec plugin",
    "short_name": "Rapid7 AppSec Plugin",
    "description": "Rapid7 AppSec Plugin",
    "version": "4.2.6",
    "manifest_version": 2,
    "incognito": "split",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./packages/browser-polyfill/browser-polyfill.min.js",
                "./contentScript.js"
            ],
            "all_frames": true,
            "run_at": "document_start",
            "persistent": "true"
        }
    ],
    "permissions": [
        "<all_urls>",
        "activeTab",
        "cookies",
        "notifications",
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "background",
        "debugger"
    ],
    "icons": {
        "128": "browser/images/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "browser/images/icon.png"
        },
        "default_title": "Rapid 7 Appsec Plugin",
        "default_popup": "browser/index.html"
    },
    "web_accessible_resources": [
        "packages/*.js",
        "packages/*.map",
        "packages/*.png",
        "dictionary/*.json",
        "patterns/*.json",
        "contentScript.js",
        "injected.min.js",
        "alf-bundle.min.js",
        "packages/alf/app.js",
        "packages/alf/images/*",
        "packages/alf/notification.js",
        "packages/alf/notification.css"
    ]
}
