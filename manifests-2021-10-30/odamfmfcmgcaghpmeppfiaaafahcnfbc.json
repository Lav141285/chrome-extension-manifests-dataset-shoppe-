---
slug: synology-office-extension
name: Synology Office Extension
publisher: Synology.com
rating: 3.4
rating_count: 10
user_count: 30,000+
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "version": "2.0.2",
  "description": "__MSG_ext_description__",
  "default_locale": "en",
  "icons": {
    "16": "images/Office_16.png",
    "48": "images/Office_48.png",
    "128": "images/Office_128.png"
  },
  "background": {
    "persistent": false,
    "page": "/html/background.html"
  },
  "permissions": [
    "<all_urls>",
    "clipboardRead",
    "storage",
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "/js/loader.js"
      ]
    }
  ],
  "options_ui": {
    "page": "/html/options.html",
    "chrome_style": true
  },
  "page_action": {
    "default_icon": {
      "16": "images/Office_16.png",
      "48": "images/Office_48.png",
      "128": "images/Office_128.png"
    }
  },
  "options_page": "/html/options.html",
  "web_accessible_resources": [
    "/js/synofficeExt.js",
    "/css/options.css",
    "/image/1x/grid_action_delete.png",
    "/image/2x/grid_action_delete.png"
  ]
}
