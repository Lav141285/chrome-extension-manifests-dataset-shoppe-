---
slug: podcastle-ai
name: Podcastle AI
publisher: Podcastle Inc.
rating: 4.101010101010101
rating_count: 99
user_count: 100,000+
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "version": "0.4.1",
  "short_name": "Podcastle",
  "name": "Podcastle AI",
  "description": "Podcastle converts text news/articles to a podcast, with very natural human speech using machine learning.",
  "browser_action": {
    "default_title": "Podcastle",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "*://*.podcastle.dev/",
    "*://*.podcastle.ai/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
