  "content_security_policy": "style-src 'self'; script-src 'self'; object-src 'self'",
  "icons": {
    "128": "images/128.png",
    "64": "images/64.png",
    "48": "images/48.png",
    "32": "images/32.png",
    "16": "images/16.png"
  },
  "browser_action": {
    "default_icon": "images/32.png",
    "default_title": "TYPO3 CMS - Little Helper",
    "default_popup": "popup.html"
  }
