{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Tab Cycler",
  "version": "0.2",

  "icons": {
    "128": "icon.png"
  },

  "permissions": [
    "tabs",
    "activeTab"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "commands": {
      "left" : {
          "suggested_key": {
               "default": "Alt+Shift+Left"
          },
          "description": "Cycle to left tab."
      },
      "right": {
          "suggested_key": {
              "default": "Alt+Shift+Right"
          },
          "description": "Cycle to right tab."
      }
  }
}
