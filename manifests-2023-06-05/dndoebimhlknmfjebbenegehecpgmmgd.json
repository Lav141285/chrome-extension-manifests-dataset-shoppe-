{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "HarvyAI - Professional Email Assistant",
  "manifest_version": 3,
  "key": "",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon32.png",
      "32": "icons/icon32.png"
    },
    "default_popup": "src/popup.html"
  },
  "version": "1.0.5",
  "description": "Reply your professional emails in 90% less time, seriously",
  "permissions": ["scripting", "storage"],

  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["src/gmail/jquery-3.6.1.min.js", "src/gmail/gmail.js"],
      "css": ["src/gmail/styles.css"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://mail.google.com/mail/*"],
      "run_at": "document_end",
      "js": ["src/gmail/index.js"]
    }
  ],
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "externally_connectable": {
    "matches": ["https://api.harvyai.com/*", "https://app.harvyai.com/*"]
  }
