{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Sprinklr Tracking Chrome Extension",
  "version": "4.1",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.sprinklr.com/*"],
      "css": ["/css/root.css"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "images/sprinklr_logo.png"
  },
  "action": {},
  "web_accessible_resources": [
    {
      "resources": ["/static/css/content.css", "/static/media/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["scripting", "activeTab", "declarativeContent", "cookies"],
  "host_permissions": ["*://*.sprinklr.com/"]
}
