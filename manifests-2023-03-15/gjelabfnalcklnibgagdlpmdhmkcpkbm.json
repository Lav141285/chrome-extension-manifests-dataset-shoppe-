{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Simple New Tab",
  "description": "This extension shows the current weather and news headlines in the new tab page.",
  "version": "1.0",

  "permissions": [
    "geolocation",
    "tabs"
  ],
  "page_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "https://www.youtube.com/iframe_api"
  ],
  "chrome_url_overrides" : {
    "newtab": "new.html"
  }
}
