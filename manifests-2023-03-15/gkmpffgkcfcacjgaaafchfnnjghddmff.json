{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,

  "name": "Searchable Bookmarks Manager",
  "description": "Easily search through your bookmarks, straight from your browser's address bar!",
  "version": "4.1.1",
  "short_name": "SBM",

  "commands": {
