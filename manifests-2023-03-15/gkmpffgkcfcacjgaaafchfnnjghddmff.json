  },

  "background": {
    "service_worker": "background/index.js",
    "type": "module"
  },

  "icons": {
    "16": "/images/icon-16-blue.png",
    "32": "/images/icon-32-blue.png",
    "48": "/images/icon-48-blue.png",
    "128": "/images/icon-128.png"
  },

  "permissions": ["activeTab", "bookmarks", "storage", "favicon", "tabGroups", "alarms"]
}
