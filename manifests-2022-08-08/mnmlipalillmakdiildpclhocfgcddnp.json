slug: rapid7-appsec-plugin
name: Rapid7 AppSec plugin
publisher: Rapid7
rating: 3.6666666666666665
rating_count: 3
user_count: 6,000+
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "author": "Rapid7",
    "name": "Rapid7 AppSec plugin",
    "short_name": "Rapid7 AppSec Plugin",
    "description": "Rapid7 AppSec Plugin",
    "version": "4.4.1",
    "manifest_version": 3,
    "incognito": "split",
    "background": {
        "service_worker": "app.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./packages/browser-polyfill/browser-polyfill.min.js",
                "./contentScript.js"
            ],
            "all_frames": true,
            "run_at": "document_start",
            "persistent": "true"
        }
    ],
    "permissions": [
        "activeTab",
        "cookies",
        "notifications",
        "storage",
        "tabs",
        "webRequest",
        "background",
        "debugger",
        "scripting",
        "management"
    ],
    "host_permissions": [
        "*://*/*",
        "file://*/*"
    ],
    "icons": {
        "128": "browser/images/icon.png"
    },
    "action": {
        "default_icon": {
            "128": "browser/images/icon.png"
        },
        "default_title": "Rapid 7 Appsec Plugin",
        "default_popup": "browser/index.html"
    },
    "web_accessible_resources": [{
        "resources": [
            "packages/*.js",
            "packages/*.map",
            "packages/*.png",
            "dictionary/*.json",
            "patterns/*.json",
            "contentScript.js",
            "injected.min.js",
            "alf-bundle.min.js",
            "packages/alf/app.js",
            "packages/alf/images/*",
            "packages/alf/notification.js",
            "packages/alf/notification.css",
            "appScript.js"
        ],
        "matches": ["<all_urls>"]
    }]
}
