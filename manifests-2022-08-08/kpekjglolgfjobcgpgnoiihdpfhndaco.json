{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "Donkhouse HUD",
	"version": "0.9.5",
	"permissions": ["storage"],
	"content_security_policy": "default-src 'self'",
	"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
	
