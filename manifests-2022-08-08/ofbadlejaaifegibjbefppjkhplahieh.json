{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Evoke Music: Royalty free music from YouTube",
  "description": "Evoke Music for Chrome, to search for music from YouTube by video tags",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "contextMenus",
    "declarativeContent",
    "http://youtube.com/",
    "https://youtube.com/"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.youtube.com/*", "https://*.youtube.com/*"],
      "js": ["src/youtube.js"],
      "css": ["src/youtube.css"]
    }
  ],
  "icons": {
    "16": "icon16px.png",
    "32": "icon32px.png",
    "48": "icon48px.png",
    "128": "icon128px.png"
  },
  "page_action": {
    "default_title": "Right click to open"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "default_locale": "en"
}
