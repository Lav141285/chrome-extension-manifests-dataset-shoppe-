            ],
            "css": ["popup/popup.css"],
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        "popup/stiqur_logo_fit.png",
        "popup/stiqur_load.svg",
        "popup/ticksy.png",
        "popup/twitter.svg",
        "*.woff2","*.woff"
    ],
