    "name": "Ticksy",
    "description": "See real-time stock and crypto prices by hovering over Twitter cashtags.",
    "version": "1.5.0",
    "icons": {
        "32": "favicon-32x32.png"
    },
    "browser_action": {
        "default_title": "Ticksy",
