{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Reachfluencer Extension",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Chrome extension to update Social information",
  "icons": {
    "16": "icons/logo-16.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/logo-19.png",
      "38": "icons/logo-38.png"
    },
    "default_title": "Reachfluencer",
    "default_popup": "src/browser_action/popup.html"
  },
  "permissions": [
    "cookies",
    "*://*.app.reachfluencer.com/",
    "*://*.back.reachfluencer.com/",
    "*://*.reachfluencer.com/",
    "*://*.linkedin.com/",
    "*://*.twitter.com/",
    "*://*.instagram.com/",
    "*://*.quora.com/",
    "*://*.medium.com/",
    "*://*.pinterest.com/",
    "*://*.producthunt.com/",
    "alarms"
  ]
}
