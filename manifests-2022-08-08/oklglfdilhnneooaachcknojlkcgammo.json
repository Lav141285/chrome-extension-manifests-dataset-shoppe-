{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PhotoKit",
  "version": "1.0.3",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_pluginDesc__",
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
