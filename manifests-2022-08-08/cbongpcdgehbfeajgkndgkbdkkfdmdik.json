{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "IMDB Search",
    "description": "Add 'imdb' keyword to omnibox for searching on IMDB, with immediate search suggestions!",
    "version": "2",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "omnibox": {
        "keyword": "imdb"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png"
    },
    "permissions": [
        "*://*.imdb.com/*",
        "*://*.media-imdb.com/*"
    ]
}
