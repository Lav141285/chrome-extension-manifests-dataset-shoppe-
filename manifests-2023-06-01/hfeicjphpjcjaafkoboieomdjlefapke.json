  "manifest_version": 3,
  "permissions": ["tabs", "storage", "cookies", "unlimitedStorage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "logo-icon.png",
    "48": "logo-icon.png",
    "128": "logo-icon.png"
