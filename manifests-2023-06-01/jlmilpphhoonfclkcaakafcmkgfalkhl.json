---
slug: cryptotab-start
name: CryptoTab START
description: >-
  New tab page extension for your browser with bunch of beautiful backgrounds
  and custom search engine box.
version: 1.2.2
release_date: 'March 2, 2023'
publisher: СryptoСompany OU
category: Productivity
category_slug: ext/7-productivity
rating: 4.71393001345895
rating_count: 14860
user_count: '300,000+'
publisher_site: cryptotabstart.com
extension_website: 'https://cryptotabstart.com/'
---

{
   "background": {
      "scripts": [ "js/background.js" ],
      "persistent": true
   },
   "browser_action": {
       "default_icon": {
          "16": "images/icon-16px.png",
          "32": "images/icon-32px.png",
          "128": "images/icon-128px.png"
       },
      "default_title": "__MSG_browserActionTitle__"
   },
   "default_locale": "en",
   "chrome_url_overrides" : {
      "newtab": "newtab.html"
   },
   "content_security_policy": "script-src 'self' https://cryptotabstart.com/;object-src 'self'",
   "description": "__MSG_extDescription__",
   "icons": {
       "128": "images/cryptotab-start_icon-128.png",
       "16": "images/cryptotab-start_icon-16.png",
       "32": "images/cryptotab-start_icon-32.png",
       "48": "images/cryptotab-start_icon-96.png"
   },
   "manifest_version": 2,
   "name": "__MSG_extName__",
   "permissions": [ "storage", "*://*.cryptobrowser.site/*", "*://cryptotabstart.com/*", "tabs", "bookmarks", "history", "chrome://favicon/*", "browsingData", "management" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.2"
}
