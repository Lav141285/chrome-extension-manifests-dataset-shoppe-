  "manifest_version": 2,

  "name": "BrainyBuzz",
  "description": "This extension facilitates a quick navigation to BrainyBuzz video catalog",
  "version": "1.0",
  
  "permissions": ["https://brainybuzz.net/", "storage", "webview", "<all_urls>"],
