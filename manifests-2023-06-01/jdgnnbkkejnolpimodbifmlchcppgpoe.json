  "manifest_version": 2,

  "name": "Puppy Overload",
  "short_name": "Puppy Ovrld",
  "description": "This extension returns an overload of labrador puppies.",
  "version": "2.1",

  "permissions": [
    "https://secure.flickr.com/",
    "activeTab"
