{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "MaterialTube Dark",
  "description": "This extension makes youtube design to Material UI Dark Mode",
  "version": "1.0.1",
  "icons": {
          "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png" 
  },
  "content_scripts": [
  {
    "matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
    "css": ["css/mystyles.css"],
    "js": ["js/myscript.js"]
  }
  ]
