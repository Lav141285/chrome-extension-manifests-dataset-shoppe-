  "name": "Deals Tracker for eBay",
  "version": "1.0.3",
  "permissions": [
    "declarativeContent",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "*://*.ebay.at/",
    "*://*.ebay.com.au/",
    "*://*.ebay.ca/",
