{
  "manifest_version": 2,
  "version": "0.3.4.1",
  "name": "Classwork Zoom for Google Classroom\u2122",
  "description": "Extension for Google Classroom\u2122",
  "content_security_policy": "script-src 'self' 'unsafe-eval'  https://apis.google.com; object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "https://docs.google.com/*",
    "https://sites.google.com/*",
    "https://classworkzoom.com/*",
    "https://drive.google.com/*",
    "tabs",
    "storage",
    "cookies",
    "system.memory",
    "https://myaccount.google.com/email",
    "https://accounts.google.com/ServiceLogin"
  ],
  "background": {
    "scripts": [
      "js/computerSpecs.js",
      "js/bg.js"

    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "images/icon32.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    },
    "default_title": "Classwork Zoom for Google Classroom\u2122"
  },
  "icons": {
    "32": "images/icon32.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "options_page": "classroom.html",
  "externally_connectable": {
    "matches": [
      "https://classworkzoom.com/*",
      "http://51.15.38.37/*"
    ]
  },
  "web_accessible_resources": ["classroom.html"]
}
