  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["activeTab", "tabs"],
  "icons": {
    "16": "icons/tag16.png",
    "48": "icons/pen-icon48.png",
    "128": "icons/pen-icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/tag16.png",
    "default_popup": "popup.html"
