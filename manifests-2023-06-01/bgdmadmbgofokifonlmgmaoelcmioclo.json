"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "TFS Finder",
  "description": "This extension open up the provided TFS Ticket in a new Tab.",
  "version": "1.0",
  "icons": { "128": "icon.png"},
  "browser_action": {
    "default_icon": "iconbar.png",
    "default_popup": "TFS.html"
