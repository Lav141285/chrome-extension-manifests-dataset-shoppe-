      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/cclogo16.png",
        "32": "images/cclogo32.png",
        "48": "images/cclogo48.png",
        "128": "images/cclogo128.png"
      }
    },
    "icons": {
      "16": "images/cclogo16.png",
        "32": "images/cclogo32.png",
        "48": "images/cclogo48.png",
        "128": "images/cclogo128.png"
    },

    "manifest_version": 2
