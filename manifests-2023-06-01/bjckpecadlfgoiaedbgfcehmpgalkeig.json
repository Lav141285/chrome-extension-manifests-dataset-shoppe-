{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Kick Them Out",
  "version": "0.5",
  "permissions": [
    "http://www.mitbbs.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.mitbbs.com/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "permissions": ["storage"],
  "icons": { "16": "icon16.png",	
             "48": "icon48.png",
            "128": "icon128.png" },
  "manifest_version": 2
}