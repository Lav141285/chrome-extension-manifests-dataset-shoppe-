{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "3DSwYm Desktop Notification",
  "version": "1.9",
  "manifest_version": 2,
  "description": "Desktop Notification extension for 3DSWYM community, specific to DSSwYm and 3DSwYm communities",
  "permissions": [
	"tabs",
	"webRequest",
	"background",
	"webNavigation",
    "https://*/.3ds.com",
	"https://*/.3ds.com/*",
	"storage",
	"notifications"
  ],
  "options_page": "mode.html",
  "web_accessible_resources": [
		"swym-Question.png",
		"swym-post.png",
		"IQuestion.png",
		"Post.jpeg",
		"authorize.jpg",
		"js/jquery-3.0.0.min.js",
		"css/jquery-ui.css",
		"https://*/*"
	],
  "background": {
	"scripts": ["js/jquery-3.0.0.min.js","entities.js","notification.js", "mode.js"]
  },
  "content_security_policy": "img-src 'self' https://*.3ds.com ; object-src 'self'; script-src 'self'"
}
