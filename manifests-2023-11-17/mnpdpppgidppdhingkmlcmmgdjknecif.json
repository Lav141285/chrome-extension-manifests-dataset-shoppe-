{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Go2Doc",
    "description": "This extension opens the corresponding tab in godoc.org.",
    "version": "0.5",

    "background": {
        "scripts": ["godoc.js"],
        "persistent": false
    },

    "permissions": [
        "activeTab",
        "declarativeContent"
    ],

    "icons": {
        "128": "doc.png"
    },

    "page_action": {
        "default_icon": "doc.png",
        "default_title": "open in godoc.org"
    }
}
