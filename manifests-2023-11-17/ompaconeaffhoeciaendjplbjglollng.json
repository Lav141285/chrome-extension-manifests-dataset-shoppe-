  "background": {
    "scripts": [
      "background/index.js"
    ]
  },
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    "common/icons/**/*",
    "**/index.html",
    "**/index.js.map",
    "common/public/*",
    "common/public/**/*"
  ],
  "homepage_url": "https://www.ipburger.com",
  "permissions": [
    "storage",
    "proxy",
    "<all_urls>",
    "privacy",
    "webNavigation",
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "browsingData"
