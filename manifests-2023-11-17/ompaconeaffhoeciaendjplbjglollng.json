  "name": "IPBurger Proxy Manager",
  "description": "IPBurger's Fresh, Dedicated and Unlimited VPN directly in Chrome",
  "version": "2.2.0",
  "manifest_version": 2,
  "icons": {
    "48": "common/icons/logo-red.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "common/icons/logo-white.png"
    },
