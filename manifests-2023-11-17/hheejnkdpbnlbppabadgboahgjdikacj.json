{
    "version": "1.2.7",
    "manifest_version": 2,
    "default_locale": "en",
    "description": "__MSG_appDesc__",
    "name": "__MSG_appName__",
    "icons": {
        "128": "images/on.png",
        "48":  "images/on48.png",
        "16":  "images/on16.png"
