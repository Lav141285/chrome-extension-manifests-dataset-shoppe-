    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "/res/img/pdf_bookmark_1.png",
      "default_title": "PDF_bookmark"
    },
    
    "permissions": ["tabs","storage","<all_urls>","https://extensionpay.com/*"]
