    },
    "duplicate_tab": {
      "suggested_key": {
        "default": "Alt+Shift+O"
      },
      "description": "Duplicate to a new window"
    },
    "close_tab": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      },
      "description": "Pop highlighted tabs out to a new window"
    },
    "random_tab": {
      "suggested_key": {
        "default": "Alt+Shift+R"
      },
      "description": "New tab to the right"
    }
