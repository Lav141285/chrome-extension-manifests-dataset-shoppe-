   "https://*.amazon.sa/*",
   "https://*.amazon.it/*",
   "https://*.amazon.com.br/*",
   "https://*.amazon.ca/*",
   "https://*.amazon.nl/*",
   "https://*.amazon.de/*",
   "https://*.amazon.se/*",
   "https://*.amazon.ae/*"
  ],
  "js": ["jquery-3.5.1.min.js", "content.js"]
 }],
 "browser_action": {
  "default_popup": "popup.html",
  "default_title": "PrimeSkipper"
 },
