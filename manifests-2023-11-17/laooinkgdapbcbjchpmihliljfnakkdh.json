  "name": "cssPicker",
  "description": "copy css from any website",
  "version": "1.0.10",
  "manifest_version": 3,
  "icons": {
    "16": "img/logo-16.png",
    "32": "img/logo-34.png",
    "48": "img/logo-48.png",
