{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "The Times Content Inspector",
  "description": "This tool for The Times and Sunday Times shows information about articles on the website (thetimes.co.uk) including their IDs",
  "version": "3.2.1",
  "icons": {
    "16": "icon-16.png",
    "19": "icon-19.png",
    "38": "icon-38.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
      "./contentListener.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.thetimes.co.uk/*",
        "http://preview.thetimes.co.uk/*",
        "http://cps-render-prod.elb.tnl-prod.ntch.co.uk/"
      ],
      "js": [
        "./content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "injectScript.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "19": "icon-19.png",
      "38": "icon-38.png",
      "128": "icon-128.png"
    },
    "default_popup": "./index.html"
  }
}