{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "MastRu Screen Sharing",
  "author": "Magora Team",
  "version": "0.2",
  "minimum_chrome_version": "34",
  "description": "This Chrome extension is developed for MastRu by Magora Team",
  "homepage_url": "https://mastru-uat.magora.team/",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://mastru-uat.magora.team/*",
      "http://localhost:2408/*"
    ]
  },
  "browser_action": {
    "default_icon": "./icons/icon.png"
  },
  "icons": {
    "16": "./icons/logo16.png",
    "48": "./icons/logo48.png",
    "128": "./icons/logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
