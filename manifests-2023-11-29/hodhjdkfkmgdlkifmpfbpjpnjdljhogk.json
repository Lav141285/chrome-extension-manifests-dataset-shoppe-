  "icons": { "48": "icon48.png", "128": "icon128.png" },
  "action": {
    "default_popup": "index.html",
    "default_title": "Kaimon Extension"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      "https://*.kaimon.io/*",
      "https://localhost:3000/*",
      "localhost:3000/*",
      "*://localhost/*"
    ]
  }
