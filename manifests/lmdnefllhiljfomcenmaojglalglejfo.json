  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "SEO Extractor",
    "default_icon": "assets/images/icon-main-48.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    "assets/styles/styles-content.css"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "assets/styles/styles-content.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
