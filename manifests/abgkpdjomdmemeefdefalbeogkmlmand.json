"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Property Tracker",
  "version": "1.1.17",
  "icons": { "128": "icon128.png" },
  "description": "Tracks changes to property prices on the RightMove website.",
