      "css": ["ptrack.css", "jquery.tooltip.css"],
      "js": ["jquery.js","jquery.tooltip.min.js", "ptrack.js"]
    }
  ],
  "web_accessible_resources": [
	"*.png"
  ],
  "permissions": [
	"tabs", 
    "https://us-central1-housetrack-uk.cloudfunctions.net/",
    "https://codemwnci.azurewebsites.net/"
  ]
