    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "image/logo.png",
        "32": "image/logo.png",
        "64": "image/logo.png",
        "128": "image/logo.png"
      }
    }
