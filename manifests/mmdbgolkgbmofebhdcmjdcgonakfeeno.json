{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Pxlet Bookmarklet",
    "version": "1.1.0",
    "description": "This is an extension to help you bookmark any link into Pxlet.com",
    "manifest_version": 2,
    "icons": {
      "16": "image/logo.png",
      "32": "image/logo.png",
      "64": "image/logo.png",
      "128": "image/logo.png"
    },
    "permissions": [
      "contextMenus", 
      "declarativeContent",
      "notifications",
      "storage",
      "tabs",
