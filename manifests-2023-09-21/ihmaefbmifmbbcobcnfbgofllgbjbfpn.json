  "optional_permissions": [
    "privacy"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "assets/logo/mycena_16x16.png",
    "32": "assets/logo/mycena_32x32.png",
    "48": "assets/logo/mycena_48x48.png",
