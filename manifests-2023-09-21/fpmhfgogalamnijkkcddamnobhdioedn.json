    "/images/Jcrop.gif",
    "/images/pixel.png"
  ],

  "commands": {
    "take-screenshot": {
      "suggested_key": {
        "default": "Alt+S"
      },
      "description": "Take Screenshot"
    }
  },

  "icons": {
    "16"  : "images/icon16.png",
    "48"  : "images/icon48.png",
    "128" : "images/icon128.png"
