{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Chat AI Search: AI results, Yahoo速 & more",
  "description": "Get search results from popular sources using quick commands from the browser address bar offered by Chat AI Search",
  "version": "1.0.0.3",
  "manifest_version": 3,
  "short_name": "Chat AI Search: AI results, Yahoo速 & more",
  "host_permissions": [
    "*://chataisearch.com/*"
  ],
  "permissions": [
    "storage",
    "contextMenus",
    "alarms",
    "declarativeNetRequestWithHostAccess"
  ],
  "background": {
    "service_worker": "serviceWorker/serviceWorker.js"
  },
  "icons": {
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://chataisearch.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "serviceWorker/storageReplacer.js",
        "contentScript.js"
      ]
    },
    {
      "matches": [
        "*://search.yahoo.com/*",
        "*://*.search.yahoo.com/*"
      ],
      "js": [
        "keepChanges.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon.png"
    },
    "default_title": "Chat AI Search: AI results, Yahoo速 & more",
    "default_popup": "popup/popup.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Chat AI",
      "keyword": "Chat AI",
      "favicon_url": "https://chataisearch.com/searchai/images/favicon_gpt4.ico",
      "search_url": "https://chataisearch.com/search.html?q={searchTerms}",
      "suggest_url": "https://chataisearch.com/apps/getChatGptSuggestion?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  }
}
