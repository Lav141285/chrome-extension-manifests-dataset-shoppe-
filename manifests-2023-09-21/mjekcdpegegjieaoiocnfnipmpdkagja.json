    "page": "./dist/options/index.html",
    "open_in_tab": true,
    "chrome_style": false
  },
  "icons": {
    "16": "./assets/icon-48.png",
    "48": "./assets/icon-48.png",
    "128": "./assets/icon-128.png"
  },
  "permissions": [
    "cookies",
    "http://*/",
    "https://*/"
  ]
