        "default_icon": "drawing.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested-key": {
                "default": "Ctrl+Shift+U"
            }
        }
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
