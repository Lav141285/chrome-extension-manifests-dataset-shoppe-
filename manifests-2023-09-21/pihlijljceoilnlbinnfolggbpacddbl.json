   "run_at": "document_end"
  }
 ],
 "background": {
  "service_worker": "background.js"
 },
 "web_accessible_resources": [
  { 
  "matches": ["<all_urls>"],
  "resources": ["images/*.png","html/login.html","html/dashboard.html"]
   }
 ],
