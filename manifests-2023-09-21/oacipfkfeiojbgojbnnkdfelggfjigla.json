        "https://*.aliexpress.com/"
    ],
    "action": {
        "default_icon": "resources/image/icon-disabled.png"
    },
    "web_accessible_resources": [{
        "resources": ["resources/scripts/jquery.min.js", "resources/scripts/wk_inserted.js","resources/image/*.png"],
        "matches": [ "https://*/*"]
    }],

    "content_scripts": [{
        "matches": [
            "https://*.aliexpress.com/*",
            "http://*.aliexpress.com/*"
        ],
        "js": ["resources/scripts/jquery.min.js", "resources/scripts/content.js"],
        "css": ["resources/css/dropship.css"]
    }]

