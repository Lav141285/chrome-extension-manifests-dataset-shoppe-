  "background": {
    "page": "app.html#/recording",
    "persistent": false
  },
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://*.storybox.blog/*"]
  },
  "browser_action": {
    "default_title": "Open Popup!"
